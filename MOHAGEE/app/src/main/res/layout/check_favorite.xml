<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/resturant_theme"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="* 소분류 (최소 택 1, 최대 택 5)"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:id="@+id/little_type_text_resturant"/>

    <Button
        android:layout_below="@+id/little_type_text_resturant"
        android:layout_marginLeft="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_marginTop="20dp"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:id="@+id/koreafood_button"
        android:background="@drawable/button_round"
        android:text="#한식"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/little_type_text_resturant"

        android:id="@+id/chinafood_button"
        android:layout_toRightOf="@+id/koreafood_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#중식"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/little_type_text_resturant"

        android:id="@+id/japanfood_button"
        android:layout_toRightOf="@+id/chinafood_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#일식"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/koreafood_button"
        android:id="@+id/westfood_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/button_round"
        android:textColor="@drawable/text_selector"

        android:text="#양식"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/koreafood_button"
        android:id="@+id/snack_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_toRightOf="@+id/koreafood_button"
        android:textColor="@drawable/text_selector"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#분식"
        />
    <Button
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:id="@+id/cafe"
        android:layout_toRightOf="@+id/snack_button"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/koreafood_button"
        android:text="#카페"
        android:layout_marginTop="10dp"
        android:background="@drawable/button_round"/>
    <Button
        android:layout_below="@+id/snack_button"
        android:layout_marginLeft="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_marginTop="20dp"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:id="@+id/billiard_button"
        android:background="@drawable/button_round"
        android:text="#당구장"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/snack_button"

        android:id="@+id/pc_room_button"
        android:layout_toRightOf="@+id/billiard_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#피시방"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/snack_button"

        android:id="@+id/singing_room"
        android:layout_toRightOf="@+id/pc_room_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#노래방"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/billiard_button"
        android:id="@+id/escape_room_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/button_round"
        android:textColor="@drawable/text_selector"

        android:text="#방탈출"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/billiard_button"
        android:id="@+id/bowling_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_toRightOf="@+id/escape_room_button"
        android:textColor="@drawable/text_selector"

        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#볼링장"
        />

    <Button
        android:layout_below="@+id/escape_room_button"
        android:layout_marginLeft="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_marginTop="20dp"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:id="@+id/movie_button"
        android:background="@drawable/button_round"
        android:text="#영화"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/escape_room_button"

        android:id="@+id/theater_button"
        android:layout_toRightOf="@+id/movie_button"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#연극"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/escape_room_button"

        android:id="@+id/Exhibition_button"
        android:layout_toRightOf="@+id/theater_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#전시회"
        />
    <Button
        android:layout_below="@+id/theater_button"
        android:layout_marginLeft="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_marginTop="20dp"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:id="@+id/shopping_button"
        android:background="@drawable/button_round"
        android:text="#쇼핑"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/theater_button"

        android:id="@+id/park_button"
        android:layout_toRightOf="@+id/shopping_button"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#공원"
        />
    <Button
        android:id="@+id/result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="#저장하기"
        android:layout_alignParentBottom="true"/>

   </RelativeLayout>